	static double[][] matrix_mult( double[][] a, double[][] b )
	{
		double[][] c = new double[a.length][b[0].length];
		int n = a[0].length;

		for( int i = 0; i < c.length; i++ )
			for( int j = 0; j < c[0].length; j++ )
				for( int k = 0; k < n; k++ )
					c[i][j] += a[i][k] * b[k][j];

		return c;
	}
